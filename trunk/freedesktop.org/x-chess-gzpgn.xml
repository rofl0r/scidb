<?xml version="1.0" encoding="utf-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
  <mime-type type="application/x-chess-gzpgn">
    <comment>Portable Game File (gzip-compressed)</comment>
    <comment xml:lang="de">PGN-Schachspielnotation (gzip-komprimiert)</comment>
    <comment xml:lang="en">Portable Game File (gzip-compressed)</comment>
    <comment xml:lang="es">Notación para juegos de ajedrez PGN (comprimido con gzip)</comment>
    <comment xml:lang="hu">PGN sakkfeljegyzés (gzip-pel tömörítve)</comment>
    <comment xml:lang="it">Notazione partita a scacchi PGN (compresso con gzip)</comment>
    <comment xml:lang="sv">PGN-schackpartinotation (gzip-komprimerat)</comment>
    <magic priority="50">
      <match type="string" offset="0" value="\x1F\x8B"/>
    </magic>
    <generic-icon name="package-x-generic"/>
    <sub-class-of type="application/x-gzip"/>
    <glob pattern="*.pgn.gz" />
  </mime-type>
</mime-info>
